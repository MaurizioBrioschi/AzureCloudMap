{
    "name": "ridesoft/azurecloudmap",
    "description": "PHP Utility library to interface with Microsoft Windows Azure Cloud API filesystem that works as in Laravel 4.2 as in pure php.",
    "keywords": ["php", "laravel","microsoft","azure cloud"],
    "license": "MIT",
    "authors": [
        {
            "name": "ridesoft",
            "email": "maurizio.brioschi@ridesoft.org"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    ],
    "require": {
        "pear-pear.php.net/mail_mime" : "*",
        "pear-pear.php.net/http_request2" : "*",
        "pear-pear.php.net/mail_mimedecode" : "*",
        "microsoft/windowsazure": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*"
    },
    "autoload": {
        "psr-0": {
            "ridesoft\\AzureCloudMap": "src/"
        }
    }
}
